  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "expect": "^1.20.2",
    "mocha": "^3.4.2"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "eventemitter3": "^2.0.2",
    "graph-crdt": "^0.7.0",
    "is-promise": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PsychoLlama/mytosis.git"
  },
