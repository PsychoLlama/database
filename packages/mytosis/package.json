{
  "name": "mytosis",
  "version": "1.15.0",
  "description": "Peer-replicated graph database",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/ -d dist/",
    "prepublish": "npm run build",
    "dev": "npm run build -- --watch",
